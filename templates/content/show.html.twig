<div>
<div class="form-group">
<label>Name</label>
<input type="text" name="name" value="{{content.name}}" />
</div>
<div class="form-group">
<label>Code</label>
<input type="text" name="name" value="{{content.code}}" />
</div>
<div class="form-group">
<label>Slug</label>
<input type="text" name="name" value="{{content.slug}}" />
</div>
{% for parameter in content.contentParameters %}
<div class="form-group">
{% for value in parameter.values %}
<label>{{value.type}}</label>
<input type="text" name="name" value="{{value}}" />
{% endfor %}
</div>
{% endfor %}
</div>